[push]
    default = simple
[core]
    editor = vim
[alias]
    df = diff
    st = status
    co = commit
    p = push
    ch = checkout
    chb = checkout -b
    br = branch
    dlb = branch -d
    dlbh = branch -D
    drb = push origin --delete
    lg = log --graph --branches --remotes --pretty=format:'%C(red)%h%Creset -%C(yellow)%d%Creset %s %C(green)(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    pu = pull
    pur = pull --rebase
    puru = pull upstream master --rebase
[credential]
    helper = cache
